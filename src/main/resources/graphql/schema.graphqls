# Tipos de consulta y mutaci√≥n

type Query {
    getUsers: [User]
    getUser(userId: String!): User
}

type Mutation {
    createUser(user: UserInput!): User
    updateUser(userId: String!, user: UserInput): User
    deleteUser(userId: String!): Boolean
}

# Tipo de objeto principal

type User {
    userId: String!
    email: String!
}

# Tipo de entrada para las mutaciones

input UserInput {
    email: String!
}